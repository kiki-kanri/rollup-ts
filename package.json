{
  "name": "ts-project-builder",
  "version": "0.9.0",
  "description": "Use rollup to build typescript project.",
  "license": "MIT",
  "author": "kiki-kanri",
  "homepage": "https://github.com/kiki-kanri/ts-project-builder#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiki-kanri/ts-project-builder.git"
  },
  "bugs": {
    "url": "https://github.com/kiki-kanri/ts-project-builder/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "bin": "./dist/cli.js",
  "files": [
    "./dist/**/*"
  ],
  "scripts": {
    "build": "node --loader \"@esbuild-kit/esm-loader\" ./src/cli.ts -c --build-type package --no-preserve-modules ./src/cli.ts ./src/index.ts"
  },
  "dependencies": {
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-strip": "^3.0.2",
    "cleye": "^1.3.2",
    "colorette": "^2.0.20",
    "pretty-ms": "^8.0.0",
    "rollup": "^3.28.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-node-externals": "^6.1.1",
    "rollup-plugin-ts": "^3.4.4",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@esbuild-kit/esm-loader": "^2.5.5",
    "@kikiutils/tsconfigs": "^0.2.1",
    "@types/node": "^20.5.7",
    "type-fest": "^4.3.1"
  },
  "keywords": [
    "build",
    "builder",
    "rollup",
    "typescript"
  ]
}