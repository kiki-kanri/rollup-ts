{
  "name": "ts-project-builder",
  "version": "0.4.2",
  "description": "Use rollup to build typescript project.",
  "license": "MIT",
  "author": "kiki-kanri",
  "homepage": "https://github.com/kiki-kanri/ts-project-builder#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiki-kanri/ts-project-builder.git"
  },
  "bugs": {
    "url": "https://github.com/kiki-kanri/ts-project-builder/issues"
  },
  "type": "commonjs",
  "main": "./dist/index.js",
  "bin": "./dist/cli.js",
  "files": [
    "./dist/**/*"
  ],
  "scripts": {
    "build": "ts-node --transpile-only ./src/cli.ts -c --build-type package --no-preserve-modules ./src/cli.ts ./src/index.ts",
    "dev": "nodemon --transpile-only ./src/cli.ts -c --build-type package --no-preserve-modules ./src/cli.ts"
  },
  "dependencies": {
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-strip": "^3.0.2",
    "cleye": "^1.3.2",
    "colorette": "^2.0.20",
    "rollup": "^3.28.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-node-externals": "^6.1.1",
    "rollup-plugin-node-externals5": "npm:rollup-plugin-node-externals@5.1.3",
    "rollup-plugin-ts": "^3.4.4",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@kikiutils/tsconfigs": "^0.1.1",
    "@types/node": "^20.5.6",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "type-fest": "^4.3.1"
  },
  "keywords": [
    "build",
    "builder",
    "rollup",
    "typescript"
  ]
}