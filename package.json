    "@rollup/plugin-typescript": "^11.1.6",
    "cleye": "^1.3.2",
    "colorette": "^2.0.20",
    "glob": "^11.0.0",
    "lodash-es": "^4.17.21",
    "pretty-ms": "^9.1.0",
    "rollup": "^4.19.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-node-externals": "^7.1.2"
  },
  "devDependencies": {
    "@kikiutils/prettierrc": "^1.3.2",
    "@kikiutils/tsconfigs": "^3.0.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.12",
